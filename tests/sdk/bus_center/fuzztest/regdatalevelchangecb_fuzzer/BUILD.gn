#####################hydra-fuzz###################
import("//build/test.gni")
import("../../../../../dsoftbus.gni")

##############################fuzztest##########################################

ohos_fuzztest("RegDataLevelChangeCbFuzzTest") {
  module_out_path = dsoftbus_fuzz_out_path
  fuzz_config_file = "$dsoftbus_root_path/tests/sdk/bus_center/fuzztest/regdatalevelchangecb_fuzzer"
  include_dirs = [
    "$dsoftbus_root_path/adapter/common/include",
    "$dsoftbus_root_path/core/common/include",
    "$dsoftbus_root_path/interfaces/inner_kits/lnn",
    "$dsoftbus_root_path/interfaces/kits/bus_center",
    "$dsoftbus_root_path/interfaces/kits/common",
    "$dsoftbus_root_path/tests/sdk/common/include",
  ]
  cflags = [
    "-g",
    "-O0",
    "-Wno-unused-variable",
    "-fno-omit-frame-pointer",
    "-fstack-protector-strong",
  ]
  sources = [ "regdatalevelchangecb_fuzzer.cpp" ]

  deps = [
    "$dsoftbus_root_path/tests/sdk:softbus_client_static",
    "$dsoftbus_root_path/tests/sdk/common:softbus_access_token_test",
  ]

  external_deps = [
    "c_utils:utils",
    "hilog:libhilog",
  ]
}
